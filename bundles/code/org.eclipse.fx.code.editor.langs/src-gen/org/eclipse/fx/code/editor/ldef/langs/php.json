          ]
        },
        {
          "$gtype": "Token",
          "defaultToken": false,
          "name": "php_compile_constants",
          "tokenScannerList": [
            {
              "$gtype": "TokenScanner_Keyword",
              "keywordList": [
                "__CLASS__",
