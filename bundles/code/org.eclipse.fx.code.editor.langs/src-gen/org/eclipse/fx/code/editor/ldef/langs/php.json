        {
          "$gtype": "PartitionRule_SingleLine",
          "endSeq": "\u0027",
          "escapedBy": "\\",
          "startSeq": "\u0027"
        }
      ],
      "tokenList": [
        {
          "$gtype": "Token",
          "defaultToken": true,
          "name": "php_string",
          "tokenScannerList": []
