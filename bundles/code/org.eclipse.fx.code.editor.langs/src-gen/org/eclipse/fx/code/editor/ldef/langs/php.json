        {
          "$gtype": "Token",
          "defaultToken": false,
          "name": "php_string_inter",
          "tokenScannerList": [
            {
              "$gtype": "TokenScanner_SingleLineRule",
              "endSeq": "}",
              "startSeq": "${"
            },
            {
              "$gtype": "TokenScanner_PatternRule",
              "containmentPattern": "\\w",
              "startLength": 0,
              "startPattern": "\\$"
            }
          ]
        }
      ]
    }
  ]
