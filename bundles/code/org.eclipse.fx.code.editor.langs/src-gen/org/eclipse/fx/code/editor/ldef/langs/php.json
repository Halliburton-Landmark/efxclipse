          ]
        }
      ]
    },
    {
      "$gtype": "Partition",
      "name": "__php_multiline_comment",
      "ruleList": [
        {
          "$gtype": "PartitionRule_MultiLine",
          "endSeq": "*/",
          "startSeq": "/*"
        }
      ],
      "tokenList": [
        {
          "$gtype": "Token",
          "defaultToken": true,
          "name": "php_doc_default",
          "tokenScannerList": []
        }
      ]
    },
    {
      "$gtype": "Partition",
      "name": "__php_string",
      "ruleList": [
        {
          "$gtype": "PartitionRule_SingleLine",
          "endSeq": "\"",
          "escapedBy": "\\",
          "startSeq": "\""
