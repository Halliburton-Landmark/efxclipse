        {
          "$gtype": "Token",
          "defaultToken": false,
          "name": "php_keyword",
          "tokenScannerList": [
            {
              "$gtype": "TokenScanner_Keyword",
              "keywordList": [
                "abstract",
                "and",
                "as",
                "break",
                "callable",
                "case",
                "catch",
                "class",
                "clone",
                "const",
