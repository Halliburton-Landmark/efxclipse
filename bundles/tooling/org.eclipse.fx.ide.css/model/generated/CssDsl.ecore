    <eStructuralFeatures xsi:type="ecore:EReference" name="rulesets" upperBound="-1"
        eType="#//Ruleset" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MediaList">
    <eStructuralFeatures xsi:type="ecore:EReference" name="entry" upperBound="-1"
