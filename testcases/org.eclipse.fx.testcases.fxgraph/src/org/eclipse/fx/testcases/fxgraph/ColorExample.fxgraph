package org.eclipse.fx.testcases.fxgraph

import javafx.scene.layout.VBox
import javafx.scene.shape.Rectangle

component ColorExample {
	VBox {
		spacing : 10,
		children : [
			Rectangle {
				width : 100,
				height : 100,
				fill : "#f00"
			}, 
			Rectangle {
				width : 100,
				height : 100,
				fill : "GREEN"
			}
		]
	}
}
